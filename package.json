{
  "name": "vm-azure-func",
  "version": "1.0.0",
  "description": "List of azure functions to get data from different data sources",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "cleanLogs": "rimraf logs",
    "cleanStubs": "rimraf stubs",
    "prestart": "npm run clean && npm run build",
    "start": "func start",
    "test": "echo \"No tests yet...\"",
    "lint": "eslint ./src --ext .ts",
    "fixlint": "eslint ./src --fix",
    "deleteGithubStub": "npx ts-node .\\src\\wiremock\\deleteStubs\\deleteGitStubs.ts",
    "createGithubStub": "npx ts-node .\\src\\wiremock\\writeStubs\\writeGitStubs.ts"
  },
  "dependencies": {
    "@azure/functions": "^4.0.0",
    "axios": "^1.7.9",
    "joi": "^17.13.3",
    "qs": "^6.14.0",
    "winston": "^3.17.0",
    "wiremock-rest-client": "^1.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/joi": "^17.2.2",
    "@types/node": "^20.x",
    "@types/qs": "^6.9.18",
    "eslint": "^9.21.0",
    "rimraf": "^5.0.0",
    "typescript": "^4.0.0",
    "typescript-eslint": "^8.24.1"
  },
  "main": "dist/src/{index.js,functions/*.js}"
}
